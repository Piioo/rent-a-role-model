<div class="container user-index">
  <h1 class="text-center"><%= @city %> Role Models</h1>
  <% @users.each do |user| %>
    <div class="role-model pull-left clearfix col-md-2">
      <%= link_to user_path(user) do %>
        <%= image_tag(user.image_url, class: "profile") %>
        <%= content_tag :p, user.name %>
      <% end %>

      <%= content_tag :p, user.job %>
    </div>
  <% end %>
</div>
